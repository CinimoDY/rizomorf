{
  "name": "rizomorf",
  "version": "1.0.0",
  "description": "Personal website with DOS theme",
  "scripts": {
    "clean": "rimraf dist",
    "setup:fonts": "node scripts/setup-fonts.js",
    "build:tokens": "node scripts/build-tokens.js",
    "build": "npm run clean && npm run build:tokens && eleventy",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "serve": "eleventy --serve",
    "deploy": "node scripts/deploy.js"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "express": "^4.17.1",
    "front-matter": "^4.0.0",
    "fs-extra": "^10.0.0",
    "marked": "^4.0.0"
  },
  "devDependencies": {
    "@tokens-studio/sd-transforms": "^0.12.2",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1",
    "style-dictionary": "^3.9.2",
    "token-transformer": "^0.0.33"
  }
}
